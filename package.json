{
  "name": "homebridge-somfi-blinds",
  "displayName": "Somfy Blinds Remote",
  "description": "A Homebridge plugin to add HomeKit compatibility to Somfy RTS devices (rolling shutters, blinds, awnings, ...) requiring only a Raspberry Pi and a simple 433 MHz transmitter.",
  "version": "0.4.1",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "babel src -d lib --ignore '**/__tests__' --ignore '**/__mocks__'"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "lib"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mwraith/somfi-blinds-homebridge.git"
  },
  "keywords": [
    "homebridge-plugin",
    "raspberry",
    "pi",
    "somfy",
    "rts",
    "homekit"
  ],
  "author": "mwraith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mwraith/somfi-blinds-homebridge/issues"
  },
  "homepage": "https://github.com/mwraith/somfi-blinds-homebridge",
  "engines": {
    "homebridge": ">=1.0.0",
    "node": ">=10.17.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "pigpio": "^3.3.1",
    "@babel/core": "^7.18.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "jest": "^28.1.0"
  }
}
